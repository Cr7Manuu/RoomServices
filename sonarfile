pipeline{
    agent any
    stages{
        tools
        {
            maven "maven"
        }
        stage('SonarQube analusis'){
            
        steps{
        withSonarQubeEnv('sonarcicd')
        {
        withMaven(maven:'maven')
        {
        bat "mvn sonar:sonar"
        }    
        }    
        }
        }
    }
}
